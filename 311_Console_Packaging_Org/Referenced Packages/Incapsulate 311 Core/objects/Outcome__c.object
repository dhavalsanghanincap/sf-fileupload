    <fields>
        <fullName>Can_Close_SR__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>OR(
   ISPICKVAL(Outcome_Action__c, &quot;Close&quot;)
)</formula>
