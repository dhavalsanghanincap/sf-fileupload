    <validationRules>
        <fullName>No_Special_Chars_In_Flex_Question_Code</fullName>
        <active>true</active>
        <description>Validates for only alphanumeric, hyphen, and underscore in Flex Question Code field</description>
        <errorConditionFormula>NOT(REGEX(Flex_Question_Code__c, &quot;[a-zA-Z0-9_-]+&quot;))</errorConditionFormula>
        <errorDisplayField>Flex_Question_Code__c</errorDisplayField>
        <errorMessage>You must only use alphanumeric (a-z, 0-9), hyphen, or underscore in the Flex Question Code field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_Must_Be_Positive</fullName>
        <active>true</active>
        <description>Ensure the value in the Order field is non-negative</description>
        <errorConditionFormula>Order__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Order__c</errorDisplayField>
        <errorMessage>The value for Order must be greater than or equal to zero (0)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Start_and_Stop_Date</fullName>
        <active>true</active>
        <description>Ensure start and stop date are both complete or both empty</description>
        <errorConditionFormula>OR(
     AND(
            NOT(ISBLANK(Start_Date__c)),
            ISBLANK(Stop_Date__c)
     ),
     AND(
            ISBLANK(Start_Date__c),
            NOT(ISBLANK(Stop_Date__c))
     )
)</errorConditionFormula>
        <errorMessage>Start Date and Stop Date must both be completed or both empty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Start_and_Stop_Date_Order</fullName>
        <active>true</active>
        <description>Ensure the chronological order of start and stop dates</description>
        <errorConditionFormula>AND(
        NOT(ISBLANK(Start_Date__c)),
        NOT(ISBLANK(Stop_Date__c)),
        Stop_Date__c &lt;= Start_Date__c
)</errorConditionFormula>
        <errorMessage>Stop Date must be after Start Date</errorMessage>
    </validationRules>
</CustomObject>
