    <validationRules>
        <fullName>Ensure_SLA_Is_Positive_Value</fullName>
        <active>true</active>
        <description>Ensure the value entered for SLA is positive and less than 10 million</description>
        <errorConditionFormula>AND(
        NOT(ISBLANK(SLA__c)),
        OR(
           SLA__c &lt;= 0,
           SLA__c &gt; 10000000
        )
)</errorConditionFormula>
        <errorMessage>The value for SLA must be a positive number greater than 0 and not greater than 10 million (10000000)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ensure_Version_Number_Is_Positive</fullName>
        <active>true</active>
        <description>Ensure the value is non-negative</description>
        <errorConditionFormula>AND(
       NOT(ISBLANK(Version_Number__c)),
       Version_Number__c &lt; 0
)</errorConditionFormula>
        <errorDisplayField>Version_Number__c</errorDisplayField>
        <errorMessage>Version number must be a non-negative number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Is_Required</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(Resolution_Type__c) == &apos;&apos;</errorConditionFormula>
        <errorDisplayField>Resolution_Type__c</errorDisplayField>
        <errorMessage>Resolution Type is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Limit_Value_To_Under_366</fullName>
        <active>true</active>
        <description>If present, value should not exceed 365 days</description>
        <errorConditionFormula>AND(
       NOT(ISBLANK(Duplicate_Threshold_Period_days__c)),
       OR(
            Duplicate_Threshold_Period_days__c &lt; 0,
            Duplicate_Threshold_Period_days__c &gt; 365
       )
)</errorConditionFormula>
        <errorDisplayField>Duplicate_Threshold_Period_days__c</errorDisplayField>
        <errorMessage>Value must be greater than or equal to 0 and less than or equal to 365</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_be_between_0_and_23</fullName>
        <active>true</active>
        <errorConditionFormula>OR(Duplicate_Threshold_Period_hours__c &lt; 0,Duplicate_Threshold_Period_hours__c &gt; 23)</errorConditionFormula>
        <errorDisplayField>Duplicate_Threshold_Period_hours__c</errorDisplayField>
        <errorMessage>Duplicate threshold hours must be between 0 and 23.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Special_Chars_In_ST_Version_Code</fullName>
        <active>true</active>
        <description>Validates for only alphanumeric, hyphen, and underscore in Version Code field</description>
        <errorConditionFormula>NOT(REGEX(Version_Code__c, &quot;[a-zA-Z0-9_-]+&quot;))</errorConditionFormula>
        <errorDisplayField>Version_Code__c</errorDisplayField>
        <errorMessage>You must only use alphanumeric (a-z, 0-9), hyphen, or underscore in the Version Code field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Radius_Values</fullName>
        <active>true</active>
        <description>Ensure value for duplicate radius is non-negative, if present</description>
        <errorConditionFormula>AND(
       NOT(ISBLANK(Duplicate_Radius_feet__c)),
       Duplicate_Radius_feet__c &lt; 0
)</errorConditionFormula>
        <errorDisplayField>Duplicate_Radius_feet__c</errorDisplayField>
        <errorMessage>Duplicate radius must be greater than or equal to 0, if defined</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_SLA_Fields</fullName>
        <active>true</active>
        <description>Ensure SLA and SLA Type are either both defined or undefined</description>
        <errorConditionFormula>OR(
     AND(
            NOT(ISBLANK(SLA__c)),
            ISBLANK(TEXT(SLA_Type__c))
      ),
      AND(
             ISBLANK(SLA__c),
             NOT(ISBLANK(TEXT(SLA_Type__c)))
      )
)</errorConditionFormula>
        <errorMessage>SLA and SLA Type must both be completed or empty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Start_and_Stop_Date_Order</fullName>
        <active>true</active>
        <description>Ensure the chronological order of start and stop dates</description>
        <errorConditionFormula>AND(
        NOT(ISBLANK(Start_Date__c)),
        NOT(ISBLANK(Stop_Date__c)),
        Stop_Date__c &lt;= Start_Date__c
)</errorConditionFormula>
        <errorMessage>Stop Date must be after Start Date</errorMessage>
    </validationRules>
